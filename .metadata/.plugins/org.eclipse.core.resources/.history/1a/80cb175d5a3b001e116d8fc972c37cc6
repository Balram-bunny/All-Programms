package WhileLoop;

public class p8 {

	public static void main(String[] args) {
		// WAP to print sum of even & Odd number from 1 to 200
		int i=1;
		int oddsum=0;
		int evensum=0;
		while (i<=10)
		{
			if (i%2==0)
			{
				 oddsum = oddsum+i;
			}
			else
			{
				evensum =evensum +i;
			}
			i++;
		}
		System.out.println(oddsum);
		System.out.println(evensum);

	}

}
