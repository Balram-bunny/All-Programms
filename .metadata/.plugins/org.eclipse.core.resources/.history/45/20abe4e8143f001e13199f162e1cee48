package Array;

public class OneDimensional {

	public static void main(String[] args) {
		// Using new keyword
			int[] a=new int[5];
			System.out.println(a);
            System.out.println(a.length);
            a[0]=1;
            a[4]=9;
            a[5]=3;
            
            System.out.println(a[0]);
            System.out.println(a[5]);
	}

}
